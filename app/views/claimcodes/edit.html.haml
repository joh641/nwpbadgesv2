.container
  .content.row
    .left-column-container.column.col-md-3
    .right-column-container.column.container.col-md-8
      .header-container.row
        %h1 Edit Claim Code
      = form_tag claimcode_path(@claimcode), :method => :put do
        #code.form-group
          = label :claimcode, :code, 'Code', :class => "control-label reserve-label"
          .reserve-input
            = text_field :claimcode, :code
        #submit
          = submit_tag 'Save Changes', :class => "btn btn-primary"
